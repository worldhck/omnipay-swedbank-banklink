{
    "name": "arbory/omnipay-swedbank-banklink",
    "type": "library",
    "description": "Swedbank Payment Initiation API driver for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "swedbank",
        "banklink"
    ],
    "homepage": "https://github.com/arbory/omnipay-swedbank-banklink",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristaps Bajazitovs",
            "email": "kristaps.bajazitovs@cube.lv"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\SwedbankBanklink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\SwedbankBanklink\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "omnipay/common": "^3.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "symfony/http-client": "^7.4"
    },
    "require-dev": {
        "omnipay/tests": "^4.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
