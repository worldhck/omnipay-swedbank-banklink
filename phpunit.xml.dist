<?xml version="1.0" encoding="UTF-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="vendor/phpunit/phpunit/phpunit.xsd"
         bootstrap="vendor/autoload.php"
         colors="true"
>
    <testsuites>
        <testsuite name="Omnipay Swedbank V3 Test Suite">
            <directory>tests</directory>
        </testsuite>
    </testsuites>
    <php>
        <env name="SWEDBANK_MERCHANT_ID" value="TEST_MERCHANT"/>
        <env name="SWEDBANK_COUNTRY" value="LV"/>
        <env name="SWEDBANK_TEST_MODE" value="true"/>
        <env name="SWEDBANK_ALGORITHM" value="RS512"/>
    </php>
    <coverage/>
</phpunit>
